{
  "compilerOptions": {
    "resolveJsonModule": true,
    "lib": [
      "es6",
      "dom",
      "es2017"
    ],
    "baseUrl": "./",
    "paths": {
      "@backend/*": [
        "src/backend/*"
      ]
    },
    "target": "es5",
    "module": "commonjs",
    "outDir": "./out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "cypress"
  ]
}